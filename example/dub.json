{
    "name": "ddbctest",
    "description": "example for DB Connector for D language, similar to JDBC",
    "authors": ["Vadim Lopatin","Laeeth Isharc"],
    "homepage": "https://github.com/buggins/ddbc",
    "license": "Boost Software License (BSL 1.0)",
    "dependencies": {
        "ddbc": {"version": "~master", "path": "../"},
        "vibe-d": "~>0.8.6"
    },
    "targetType": "executable",
    "buildRequirements": [
		"allowWarnings"
	],
    "versions": ["VibeCustomMain"],
    "configurations": [
        {
            "name": "default",
            "subConfigurations": {
                "ddbc": "full"
            }
        },
        
        {
            "name": "default-omf",
            "subConfigurations": {
                "ddbc": "full-omf"
            }
        },

        {
            "name": "MySQL",
            "subConfigurations": {
                "ddbc": "MySQL"
            }
        },

        {
            "name": "SQLite",
            "subConfigurations": {
                "ddbc": "SQLite"
            }
        },

        {
            "name": "SQLite-omf",
            "subConfigurations": {
                "ddbc": "SQLite-omf"
            }
        },

        {
            "name": "PGSQL",
            "subConfigurations": {
                "ddbc": "PGSQL"
            }
        },

        {
            "name": "ODBC",
            "subConfigurations": {
                "ddbc": "ODBC"
            }
        }
    ]
}
